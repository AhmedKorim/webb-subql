{
  "name": "webb-subql",
  "version": "1.0.0",
  "description": "This project can be used as a starting point for developing your SubQuery project",
  "scripts": {
    "subql:deploy:dev": "cd apps/substrate && yarn deploy:dev",
    "ts-check": "tsc --noEmit -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "homepage": "https://github.com/subquery/subql-starter",
  "repository": "github:subquery/subql-starter",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "@webb-developers",
  "license": "MIT",
  "dependencies": {
    "@graphprotocol/graph-cli": "0.42.4",
    "@graphprotocol/graph-ts": "0.29.1"
  },
  "private": true,
  "workspaces": [
    "apps/**"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "ipfs-unixfs": "6.0.6"
  }

